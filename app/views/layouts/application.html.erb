<!DOCTYPE html>
<html>
  <head>
    <title>Practicalgifs</title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <div class="container">

      <%= render 'layouts/header'%>
      <%= yield %>
      <%#= render 'layouts/footer' %>

    </div>
    <%= render partial: 'gifs/form', layout: 'gifs/modal',  locals: {label: 'new_gif_form', title: 'New Gif'} if current_user %>
    <%= render partial: 'users/form', layout: 'layouts/modal', locals: {label: 'main_modal', title: 'New User'}  %>
  </body>
</html>
